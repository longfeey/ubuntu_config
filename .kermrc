#----- kermrc for /dev/ttySn -------------------
# ADI blackfin bf533 ezkit U_boot kermit setting
set line /dev/ttyS0
#set line /dev/ttyUSB0
define sz !sz \%0 > /dev/ttyS0 < /dev/ttyS0
set speed 115200
set carrier-watch off
set prefixing all
set parity none
set stop-bits 1
set modem none
set file type bin
set file name lit
set flow-control none
set prompt "Linux Kermit> "
#---------end------------------------------------
